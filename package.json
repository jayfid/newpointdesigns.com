{
  "name": "newpointdesigns.com",
  "version": "1.0.0",
  "description": "newpointdesigns.com source",
  "private": true,
  "engines": {
    "node": ">=16.15"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist .sass-cache",
    "pretty": "npx prettier --write .",
    "lint": "npx eslint src",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "start": "webpack serve",
    "deploy": "aws s3 cp ./dist/ 's3://www.newpointdesigns.com'  --recursive --acl public-read --cache-control public,max-age=604800",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E1V20TH9YDWKSV --paths '/*' > /dev/null",
    "ship": "npm run-script build && npm run-script deploy && npm run-script invalidate"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@webpack-cli/generators": "^2.4.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.15.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "html-webpack-partials-plugin": "^0.8.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayfid/newpointdesigns.com.git"
  },
  "author": "jayfid",
  "homepage": "https://www.newpointdesigns.com"
}
