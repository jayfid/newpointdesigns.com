{
  "name": "newpointdesigns.com",
  "version": "1.0.0",
  "description": "newpointdesigns.com source",
  "private": true,
  "engines": {
    "node": ">=0.14.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist .sass-cache",
    "pretty": "npx prettier --write .",
    "lint": "npx eslint src",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "deploy": "aws s3 cp ./dist/ 's3://www.newpointdesigns.com'  --recursive --acl public-read --cache-control public,max-age=604800",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E1V20TH9YDWKSV --paths '/*' > /dev/null",
    "ship": "npm run-script build && npm run-script deploy && npm run-script invalidate"
  },
  "dependencies": {
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@webpack-cli/generators": "^2.3.0",
    "autoprefixer": "^10.3.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "html-webpack-partials-plugin": "^0.8.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "sass": "^1.38.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "browser": true,
      "jquery": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayfid/newpointdesigns.com.git"
  },
  "author": "jayfid",
  "homepage": "https://www.newpointdesigns.com"
}
